{
    "builds": [{
        "src": "lista_de_tarefas/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],


    "routes": [
         {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "lista_de_tarefas/wsgi.py"
        }
    ]
}