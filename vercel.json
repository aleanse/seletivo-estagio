{
    "builds": [{
        "src": "lista_de_tarefas/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],


    "routes": [
         {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "lista_de_tarefas/wsgi.py"
        }
    ],
   "env": {
        "DJANGO_SETTINGS_MODULE": "lista_de_tarefas.settings"
    },
    "buildCommand": "python manage.py collectstatic"
}